{
    "require": {
        "guzzlehttp/guzzle": "^7.5",
        "ingenerator/php-utils": "^1.19",
        "psr/log": "^1",
        "symfony/filesystem": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "repositories": [
        {"type": "composer", "url": "https://php-packages.ingenerator.com"}
    ],
    "autoload": {
        "psr-4": {
            "Ingenerator\\ApiEmulator\\": "src"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "process-timeout":   600,
        "preferred-install": "dist",
        "sort-packages":     true,
        "platform": {
            "php": "8.1",
            "ext-apcu": "8.1",
            "ext-bcmath": "8.1",
            "ext-curl": "0",
            "ext-ffi": "8.1",
            "ext-gd": "0",
            "ext-json": "0",
            "ext-libxml": "0",
            "ext-mbstring": "8.1",
            "ext-openssl": "0",
            "ext-PDO": "8.1",
            "ext-grpc": "3.13",
            "ext-sodium": "8.1",
            "ext-tokenizer": "0.1",
            "ext-zip": "0"
        },
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
